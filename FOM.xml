    <objects>
        <objectClass>
            <name>HLAobjectRoot</name>
            <objectClass>
                <name>Communication</name>
                <sharing>Neither</sharing>
                <semantics>Abstract Class for NETN-Communication Objects</semantics>
                <objectClass>
                    <name>CommunicationNetworkStatus</name>
                    <sharing>PublishSubscribe</sharing>
                    <semantics>This NetworkConnectivityStatus HLA Object is created and managed during the HLA exercise by a communication manager simulation (service). 
 
The NetworkConnectivityStatus holds a list all available connectivity links between sending and receiving entities and the communication status (e.g. available or interrupted connectivity).</semantics>
                    <attribute>
                        <name>CommunicationNetworkId</name>
                        <dataType>UuidArrayOfHLAbyte16</dataType>
                        <updateType>Static</updateType>
                        <updateCondition>Initial</updateCondition>
                        <ownership>NoTransfer</ownership>
                        <sharing>Neither</sharing>
                        <transportation>HLAreliable</transportation>
                        <order>Receive</order>
                        <semantics>Identifies the communicationn network.</semantics>
                    </attribute>
                    <attribute>
                        <name>CommunicationConnectivity</name>
                        <dataType>NetworkConnectivityStructArray</dataType>
                        <updateType>Conditional</updateType>
                        <updateCondition>On Change</updateCondition>
                        <ownership>Divest</ownership>
                        <sharing>PublishSubscribe</sharing>
                        <transportation>HLAbestEffort</transportation>
                        <order>Receive</order>
                        <semantics>N/A</semantics>
                    </attribute>
                </objectClass>
            </objectClass>
        </objectClass>
    </objects>
    <dimensions/>
    <synchronizations/>
    <transportations/>
    <updateRates/>
    <dataTypes>
        <basicDataRepresentations/>
        <simpleDataTypes/>
        <enumeratedDataTypes/>
        <arrayDataTypes>
            <arrayData>
                <name>NetworkConnectivityStructArray</name>
                <dataType>NetworkConnectivityStruct</dataType>
                <cardinality>Dynamic</cardinality>
                <encoding>HLAvariableArray</encoding>
                <semantics>N/A</semantics>
            </arrayData>
        </arrayDataTypes>
        <fixedRecordDataTypes>
            <fixedRecordData>
                <name>NetworkConnectivityStruct</name>
                <encoding>HLAfixedRecord</encoding>
                <semantics>Describes the connectivity status between a sender and a receiver entity. 
The state of this communication link is decribed in percentage of the probability of having a connection: 0% if there is no connectivity. 100% if there is maximum connectivity available.</semantics>
                <field>
                    <name>Sender</name>
                    <dataType>UuidArrayOfHLAbyte16</dataType>
                    <semantics>Uniquie Id of the sending entity/unit</semantics>
                </field>
                <field>
                    <name>Receiver</name>
                    <dataType>UuidArrayOfHLAbyte16</dataType>
                    <semantics>Unique Id of the receiving entity/unit</semantics>
                </field>
                <field>
                    <name>NetworkState</name>
                    <dataType>PercentFloat32</dataType>
                    <semantics>Percentage decribing the probability of having a connection: 0% if there is no connectivity. 100% if there is maximum connectivity available.</semantics>
                </field>
            </fixedRecordData>
        </fixedRecordDataTypes>
        <variantRecordDataTypes/>
    </dataTypes>
    <notes/>
